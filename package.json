{
  "name": "frigatesimpleui-setup",
  "version": "1.0.0",
  "description": "FrigateSimpleUI Setup Web Application for managing camera configurations",
  "main": "server/index.js",
  "homepage": "./",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon server/index.js",
    "dev:client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install:all": "npm install && cd client && npm install",
    "test": "jest",
    "start:both": "concurrently \"npm run start\" \"npm run dev:client\""
  },
  "keywords": [
    "frigatesimpleui",
    "camera",
    "setup",
    "onvif",
    "rtsp"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "node-onvif": "^0.1.7",
    "onvif": "^0.8.0",
    "p-limit": "^6.2.0",
    "uuid": "^11.1.0",
    "validator": "^13.15.0",
    "winston": "^3.11.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  }
}
